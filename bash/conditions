#! /bin/bash

n1=100
if [[ $n1 -eq 100 ]];
then
        echo "both values are same"
else
        echo "values are not equal"
fi

------ 

  #! /bin/bash

n1=50
if [[ $n1 -eq 100 ]];
then
        echo "both values are same"
else
        echo "values are not equal"

fi

