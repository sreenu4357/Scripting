#! /bin/bash

n1=50
if [[ $n1 -lt 100 ]];
then
        echo "true"
else
        echo "false"

fi
________________________________

#! /bin/bash

n1=200
if [[ $n1 -gt 100 ]];
then
        echo "true"
else
        echo "false"

fi
____________________________

#! /bin/bash

n1=200
if [[ $n1 -ge 200 ]];
then
        echo "true"
else
        echo "false"

fi
___________________________________

#! /bin/bash  - empty string

name=""

if [[ -z $name ]];
then
        echo "it is  empty string"
fi

______________________

name="sreenu"

if [[ -n $name ]];
then
        echo "it is not empty string"
fi

