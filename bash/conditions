#! /bin/bash

user1="sreenu"
user2="rahul"

if [[ $user1 == $user2 ]];
then
        echo "both users are same"
else
        echo "both users are not same"
fi

___________________________-
  #! /bin/bash

user1="sreenu"
user2="rahul"

if [[ $user1 != $user2 ]];
then
        echo "both users are different"
else
        echo "both users are same"
fi
________________________________________

#! /bin/bash

user1=123
user2=123

if [[ $user1 -eq $user2 ]];
then
        echo "both numbers are same"
else
        echo "both numbers are different"
fi


